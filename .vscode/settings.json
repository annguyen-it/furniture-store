{
  "cSpell.words": [
    "Davici",
    "Vetra"
  ],
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock",
    ".env": ".env.local",
    ".prettierrc": ".prettierignore",
    "jest.config.ts": "jest.*"
  },
  "files.exclude": {
    "**/.angular": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/*.md": true,
    "**/CVS": true,
    "**/jest.*": true,
    "**/node_modules": true,
    "**/test-setup.ts": true,
    "**/Thumbs.db": true,
    "*/**/.eslintrc.json": true,
    "*/**/tsconfig.*": true,
  }
}
